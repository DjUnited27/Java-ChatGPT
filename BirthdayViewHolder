package com.example.chatgptprojectappv11;

import android.view.View;
import android.widget.TextView;

import androidx.annotation.NonNull;
import androidx.recyclerview.widget.RecyclerView;

public class BirthdayViewHolder extends RecyclerView.ViewHolder {

    TextView name;
    TextView surname;
    TextView company;
    TextView birthday;

    public BirthdayViewHolder(@NonNull View itemView) {
        super(itemView);

        name = itemView.findViewById(R.id.textview_name);
        surname = itemView.findViewById(R.id.textview_surname);
        company = itemView.findViewById(R.id.textview_company);
        birthday = itemView.findViewById(R.id.textview_birthdate);
    }
}
